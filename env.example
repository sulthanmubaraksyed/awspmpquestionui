# PMP Service Configuration
# Set this to your backend service URL (required)
REACT_APP_PMP_SERVICE_URL=your_backend_service_url_here

# API Authentication
# Set this to your API key for authentication
REACT_APP_API_KEY=your_api_key_here

# Admin Authentication
# Set this to your admin password for authentication
REACT_APP_ADMIN_PASSWORD=admin123

# Other environment variables can be added here
# REACT_APP_API_TIMEOUT=10000
# REACT_APP_DEBUG_MODE=true